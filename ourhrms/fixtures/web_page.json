  "main_section": null,
  "main_section_html": "\n\n<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>My Payslips</title>\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.0/css/bootstrap.min.css\">\n</head>\n<body>\n    <div class=\"container mt-5\">\n        <h2>My Payslips</h2>\n        <table class=\"table table-bordered\" id=\"payslipTable\">\n            <thead>\n                <tr>\n                    <th>ID</th>\n                    <th>Total Salary</th>\n                    <th>Deductions</th>\n                    <th>Net Salary</th>\n                    <th>Actions</th>\n                </tr>\n            </thead>\n            <tbody>\n                <!-- Payslip data will be inserted here -->\n            </tbody>\n        </table>\n        <p id=\"noPayslipMessage\" style=\"display: none;\">No payslips found.</p>\n    </div>\n\n    <script>\n        document.addEventListener(\"DOMContentLoaded\", function () {\n            fetchPayslips();\n        });\n\n        function fetchPayslips() {\n            fetch('/api/method/frappe.client.get_list?doctype=Payslip&fields=[\"name\",\"total_salary\",\"total_deductions\",\"net_salary\"]', {\n                credentials: \"include\"\n            })\n            .then(response => response.json())\n            .then(data => {\n                let tableBody = document.querySelector(\"#payslipTable tbody\");\n                let noPayslipMessage = document.getElementById(\"noPayslipMessage\");\n\n                if (data.message.length === 0) {\n                    noPayslipMessage.style.display = \"block\";\n                    return;\n                }\n\n                data.message.forEach(payslip => {\n                    let row = `<tr>\n                        <td>${payslip.name}</td>\n                        <td>${payslip.total_salary}</td>\n                        <td>${payslip.total_deductions}</td>\n                        <td>${payslip.net_salary}</td>\n                        <td>\n                            <a href=\"/api/method/frappe.utils.print_format.download_pdf?doctype=Payslip&name=${payslip.name}\" \n                               class=\"btn btn-primary btn-sm\" target=\"_blank\">Download PDF</a>\n                        </td>\n                    </tr>`;\n                    tableBody.innerHTML += row;\n                });\n            })\n            .catch(error => console.error(\"Error fetching payslips:\", error));\n        }\n    </script>\n\n</body>\n</html>\n",
